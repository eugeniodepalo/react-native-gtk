{
  "name": "react-native-gtk4",
  "version": "2.1.1",
  "description": "React renderer for GTK4 using node-gtk",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup-node src/index.ts",
    "lint": "eslint src",
    "test": "jest",
    "codegen": "./bin/codegen",
    "girs:copy": "ts-for-gir copy Gtk-4.0 -o ./girs -g /usr/share/gir-1.0 /usr/lib/*/gir-1.0",
    "girs:generate": "ts-for-gir generate Gtk-4.0 -o ./src/generated/girs -g ./girs -e node"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eugeniodepalo/react-native-gtk4.git"
  },
  "keywords": [
    "react",
    "gtk",
    "node-gtk",
    "native"
  ],
  "author": "Eugenio Depalo <eugeniodepalo@gmail.com>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/eugeniodepalo/react-native-gtk4/issues"
  },
  "homepage": "https://github.com/eugeniodepalo/react-native-gtk4",
  "dependencies": {
    "lodash": "^4.17.21",
    "react-reconciler": "^0.29.2",
    "remeda": "^2.5.0"
  },
  "peerDependencies": {
    "node-gtk": "^0.12.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@ts-for-gir/cli": "^3.3.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-reconciler": "^0.28.8",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-react": "^7.34.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  }
}
